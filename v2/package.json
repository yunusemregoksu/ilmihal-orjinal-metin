{
  "name": "v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "TAILWIND_MODE=watch & NODE_ENV=development & postcss ./styles/globals.css -o ./styles/main.css -w",
    "build:dev": "TAILWIND_MODE=build & NODE_ENV=development & postcss ./styles/globals.css -o ./styles/main.css",
    "build:prod": "TAILWIND_MODE=build & NODE_ENV=production & postcss ./styles/globals.css -o ./styles/main.css",
    "build": "next build",
    "start": "NODE_OPTIONS=--openssl-legacy-provider next dev"
  },
  "dependencies": {
    "@nextui-org/react": "^1.0.0-beta.12",
    "@vercel/analytics": "^0.1.11",
    "next": "11",
    "next-themes": "^0.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remixicon": "^3.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.15",
    "tailwindcss": "^3.3.2"
  }
}
